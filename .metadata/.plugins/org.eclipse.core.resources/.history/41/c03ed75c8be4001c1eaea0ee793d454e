package string;
// non repeating number
public class A {
	public static void main(String[] args) {
		String str = "abbcdda";
		String str2 = "abfgh";
		int count=0;
		for(int i=0; i<str.length(); i++) {
			char ch = str.charAt(i);
			
			System.out.print(i);
		}
		for(int j=0; j<str2.length(); j++) {
			char ch2 = str2.charAt(j);
			System.out.printl(ch2);
			
		}
	}
}
