package string;
// non repeating number
public class A {
	public static void main(String[] args) {
		String str = "abbcdda";
		boolean flag= true;
		for(char i: str.toCharArray()) {
			if(str.indexOf(i)==str.lastIndexOf(i)) {
				System.out.println("repating charter is "+i);
				flag=false;
				break;
			}
		}
		if(flag) {
			System.out.println("no reapeting string");
		}
	}
}
